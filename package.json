{
  "name": "nuxt-todo",
  "version": "1.0.0",
  "description": "A todo list manager made with Nuxt.js",
  "author": "nemoto",
  "private": true,
  "scripts": {
    "dev": "cross-env NODE_ENV=development nodemon server/index.js --watch server",
    "build": "nuxt build",
