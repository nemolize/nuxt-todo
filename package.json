    "bulma": "^0.9.0",
    "cross-env": "^7.0.2",
    "express": "^4.16.4",
    "node-sass": "^4.11.0",
    "nuxt": "^2.14.0",
    "sass-loader": "^9.0.2",
    "vuex": "^3.5.1"
  },
  "devDependencies": {
    "@nuxtjs/eslint-config": "^3.1.0",
    "@vue/test-utils": "^1.0.3",
    "babel-core": "7.0.0-bridge.0",
    "babel-eslint": "^10.1.0",
    "babel-jest": "^26.2.2",
    "eslint": "^7.6.0",
    "eslint-config-prettier": "^6.11.0",
